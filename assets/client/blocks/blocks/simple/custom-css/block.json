{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "omnipress/css",
  "title": "Custom CSS",
  "category": "omnipress",
  "icon": "",
  "description": "Add custom CSS with blocks for styling without theme changes",
  "keywords": [
    "custom",
    "css"
  ],
  "textdomain": "omnipress",
  "attributes": {
    "css": {
      "type": "string"
    },
    "id": {
      "type": "string"
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "anchor": true,
    "className": true,
    "__unstablePasteTextInline": true,
    "__experimentalSlashInserter": true
  }
}